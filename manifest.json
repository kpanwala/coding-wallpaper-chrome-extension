{
    "manifest_version": 2,
    "name": "Wallpaper coder",
    "description": "Coding wallpaper appears everytime on opening a new tab",
    "version": "1.0",
    "icons": {
      "128": "icon128.png",
      "64": "icon64.png",
      "16": "icon16.png"
    },

    "browser_action":{
      "default_icon":"icon16.png",
      "default_popup":"popup.html"
    },
    "chrome_url_overrides":{
      "newtab":"newtab.html"
    }
  
  }